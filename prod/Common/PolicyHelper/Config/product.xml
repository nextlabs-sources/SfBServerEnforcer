<ProductXml>
    <Obligations>
        <Obligation>
            <DisplayName>SFB User Condition</DisplayName>
            <RunAt>PEP</RunAt>
            <Name>SFB_User_Condition</Name>
            <Arguments>
                <Argument usereditable="false" hidden="false">
                    <Name>Subject</Name>
                    <Value default="true">Participants</Value>
                </Argument>
                <Argument usereditable="true" hidden="false">
                    <Name>Matches_Policy_With_Condition</Name>
                    <Value>userDefinedName=UserDenfinedValue</Value>
                </Argument>
            </Arguments>
        </Obligation>
        <!-- SFBE: SFB_Notify-->
        <Obligation>
            <DisplayName>Notify Skype user</DisplayName>
            <RunAt>PEP</RunAt>
            <Name>SFB_Notify</Name>
            <Arguments>
                <Argument usereditable="true" hidden="false">
                    <Name>Message</Name>
                    <Value>a notify message string</Value>
                </Argument>
            </Arguments>
        </Obligation>

        <!-- SFBE: SFB_AutoSet_Enforcement-->
        <Obligation>
            <DisplayName>Enable Enforcement</DisplayName>
            <RunAt>PEP</RunAt>
            <Name>SFB_AutoSet_Enforcement</Name>
        </Obligation>

        <!-- SFBE: SFB_Auto_Classify-->
        <Obligation>
            <DisplayName>SFB Auto Classify</DisplayName>
            <RunAt>PEP</RunAt>
            <Name>SFB_Auto_Classify</Name>
            <Arguments>
                <Argument usereditable="true" hidden="false">
                    <Name>Key</Name>
                    <Value>Classify key</Value>
                </Argument>
                <Argument usereditable="true" hidden="false">
                    <Name>Value</Name>
                    <Value>Classify value</Value>
                </Argument>
            </Arguments>
        </Obligation>

        <!-- SFBE: SFB_Manul_Classify-->
        <Obligation>
            <DisplayName>SFB Manual Classify</DisplayName>
            <RunAt>PEP</RunAt>
            <Name>SFB_Manual_Classify</Name>
            <Arguments>
                <Argument usereditable="false" hidden="true">
                    <Name>Data</Name>
                    <Value>NLClassificationName=DefaultClassification</Value>
                </Argument>
                <Argument usereditable="false" hidden="false">
                    <Name>Force_Classify</Name>
                    <Value default="true">Yes</Value>
                    <Value>No</Value>
                </Argument>
            </Arguments>
        </Obligation>

        <!-- SFBE: SFB_AutoSet_Classify_Manager -->
        <Obligation>
            <DisplayName>Enable Classify Manager</DisplayName>
            <RunAt>PEP</RunAt>
            <Name>SFB_AutoSet_Classify_Manager</Name>
        </Obligation>
    </Obligations>

    <Actions>
